# Botpress Configuration
BOTPRESS_CLIENT_ID=your-client-id-here
BOTPRESS_WEBHOOK_ID=your-webhook-id-here
BOTPRESS_BOT_ID=mindcare-ai-bot

# API Keys (if needed)
ANALYTICS_API_KEY=your-analytics-key-here
MONITORING_API_KEY=your-monitoring-key-here

# Environment
NODE_ENV=development
PORT=8000

# Security
SESSION_SECRET=your-super-secret-session-key-here
ENCRYPTION_KEY=your-32-character-encryption-key

# Crisis Intervention Settings
EMERGENCY_PHONE=988
CRISIS_TEXT_LINE=741741
EMERGENCY_SERVICES=911

# Feature Flags
ENABLE_ANALYTICS=true
ENABLE_CRISIS_DETECTION=true
ENABLE_DEBUG_MODE=false

# Deployment
DEPLOY_URL=https://mindcare-ai.netlify.app
GITHUB_REPO=Nostalgic-ash777/MindCare_AI
